<% if(@submission_successful) then %>
{
  "status" : "All submissions."
  "data" : {
             <% @submissions.each do |submission| %>
               <%= render :partial => 'submissions/submission', :locals => { :submission => submission } %>
             <% end %>
           }
}
<% else %>
{
  "status" : "Retrieval Failed, student_key not valid."
}
<% end %>
